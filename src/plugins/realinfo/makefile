# Where to find user code.

LIB_DIR = ./../../../../mylib
USER_DIR = ./../../pub
BASE_CORE = ./../../../../basecore
SO_DIR = ./../../bin/plugins
STOCKSVC = ./../../plugins
ARITHMETIC = $(USER_DIR)/arithmetic
DEPENDS_DIR=/home/runner/depends
THIRD_PATH = $(LIB_DIR)/public

CUSTOM_THIRD_PATH = $(LIB_DIR)/third_party

LINK_PATHS = -L./ -L/usr/local/libmemcached-1.0.7/lib/  -L/usr/lib64 -L/usr/lib64/mysql -L$(SO_DIR)/realinfo/

# Flags passed to the preprocessor.
CPPFLAGS += -isystem -shared -fPIC -DKUN_REDIS_POOL -D_DB_POOL_  -D_KUN_DB_POOL -D_DB_POOL_ -D_DEBUG -DUSR_LOG_MSG -DPROCESS_WORK -DUSE_MARKET -DUSE_STOCKSHARE

# Flags passed to the C++ compiler.
CXXFLAGS += -g  -Wextra -pthread -shared -fPIC -DKUN_REDIS_POOL

# All tests produced by this Makefile.  Remember to add new tests you
# created to the list.
PLUGIN = $(SO_DIR)/realinfo/realinfo.so
                
INCLUDE_PATHS = -I/usr/local/libmemcached-1.0.7/include \
                                -I$(DEPENDS_DIR)/include\
				-I/usr/local/libmemcached/include\
				-I/usr/local/freetds/include\
				-I$(LIB_DIR)\
				-I$(BASE_CORE)\
				-I$(THIRD_PATH)\
				-I/usr/local/include\
				-I/usr/include/mysql\
				-I$(USER_DIR)\
				-I$(CUSTOM_THIRD_PATH)/hiredis\
				-I$(CUSTOM_THIRD_PATH)\
				-I$(LIB_DIR)/base\
				-I$(BASE_DIR)\
				-I$(LIB_DIR)/public\
				-I$(STOCKSVC)\
				-I$(LIB_DIR)/third_party/hiredis\
				-I$(LIB_DIR)/third_party\
				-I$(ARITHMETIC)\
                                -I.\
                                -I..\
                                -I$(USER_DIR)/sqldbpool\
				-I$(USER_DIR)/baseconfig\

# House-keeping build targets.

ALL_HEADERS = $(USER_HEADERS)
all : $(PLUGIN) delete-temp-file

clean :
	rm -f $(PLUGIN)  *.o

delete-temp-file:
	-rm -f ./*.o

base_config.o : ${USER_DIR}/baseconfig/base_config.cc ${USER_DIR}/baseconfig/base_config.h
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c ${USER_DIR}/baseconfig/base_config.cc $(INCLUDE_PATHS)

mysql_db_pool.o : ${USER_DIR}/sqldbpool/mysql_db_pool.cc ${USER_DIR}/sqldbpool/mysql_db_pool.h 
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c ${USER_DIR}/sqldbpool/mysql_db_pool.cc $(INCLUDE_PATHS)

realinfo_init.o : ./realinfo_init.cc ./realinfo_init.h
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c ./realinfo_init.cc $(INCLUDE_PATHS) 


realinfo_logic.o : ./realinfo_logic.cc ./realinfo_logic.h
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c ./realinfo_logic.cc $(INCLUDE_PATHS)
realinfo_cache.o : ./realinfo_cache.cc ./realinfo_cache.h
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c ./realinfo_cache.cc $(INCLUDE_PATHS)
realinfo_today.o : ./realinfo_today.cc ./realinfo_today.h
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c ./realinfo_today.cc $(INCLUDE_PATHS)
		
http_data.o : $(USER_DIR)/net/http_data.cc $(USER_DIR)/net/http_data.h
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $(USER_DIR)/net/http_data.cc $(INCLUDE_PATHS)
	
tools.o : $(USER_DIR)/tools/tools.cc $(USER_DIR)/tools/tools.h
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $(USER_DIR)/tools/tools.cc $(INCLUDE_PATHS)
	
tea.o:$(ARITHMETIC)/tea/tea.cc $(ARITHMETIC)/tea/tea.h
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $(ARITHMETIC)/tea/tea.cc $(INCLUDE_PATHS)

error_comm.o :  $(USER_DIR)/net/error_comm.cc $(USER_DIR)/net/error_comm.h
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $(USER_DIR)/net/error_comm.cc $(INCLUDE_PATHS)
	
logic_basic_info.o :  $(USER_DIR)/logic/logic_basic_info.cc $(USER_DIR)/logic/logic_basic_info.h
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $(USER_DIR)/logic/logic_basic_info.cc $(INCLUDE_PATHS)

logic_unit.o :  $(USER_DIR)/logic/logic_unit.cc $(USER_DIR)/logic/logic_unit.h
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $(USER_DIR)/logic/logic_unit.cc $(INCLUDE_PATHS)
	
http_packet.o: $(THIRD_PATH)/protocol/http_packet.cc
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $(THIRD_PATH)/protocol/http_packet.cc $(INCLUDE_PATHS)
	
mig_log.o: $(THIRD_PATH)/log/mig_log.cc
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $(THIRD_PATH)/log/mig_log.cc $(INCLUDE_PATHS)
	
basictypes.o: $(THIRD_PATH)/basic/basictypes.cc
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $(THIRD_PATH)/basic/basictypes.cc $(INCLUDE_PATHS)
	
#basic_util.o: $(THIRD_PATH)/basic/basic_util.cc
#	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $(THIRD_PATH)/basic/basic_util.cc $(INCLUDE_PATHS)
	
config.o: $(THIRD_PATH)/config/config.cc
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $(THIRD_PATH)/config/config.cc $(INCLUDE_PATHS)
	
md5sum.o:$(THIRD_PATH)/basic/md5sum.cc
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $(THIRD_PATH)/basic/md5sum.cc $(INCLUDE_PATHS)

radom_in.o:$(THIRD_PATH)/basic/radom_in.cc
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) -c $(THIRD_PATH)/basic/radom_in.cc $(INCLUDE_PATHS)


$(PLUGIN) :  realinfo_init.o realinfo_logic.o  \
http_data.o http_packet.o mig_log.o basictypes.o error_comm.o  \
logic_unit.o config.o  \
  \
 \
md5sum.o radom_in.o \
$(CUSTOM_THIRD_PATH)/dmg_fp/lib/dmg_fp.a $(LIB_DIR)/base/lib/mybase.a $(LIB_DIR)/third_party/lib/libhiredis.a
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) $(LINK_PATHS) -lpthread -lexpat -lrt -lpthread -ldl -lmysqlclient -lcurl \
$^ -o $@  -L $(DEPENDS_DIR)/lib/market_info  -Wl,-rpath=$(DEPENDS_DIR)/lib/market_info  -lmarket_info  -L $(SO_DIR)/../stockshare -Wl,-rpath=stockshare -lstockshare
