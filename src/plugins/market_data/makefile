$(PLUGIN) :  redis_controller.o realinfo_day.o realinfo_control.o realinfo_minute.o   \
	http_packet.o mig_log.o basictypes.o   \
	logic_unit.o basic_util.o config.o  \
	md5sum.o radom_in.o  \
 market_data_engine.o  \
	stock_price.o stock_util.o realtime_code_info.o \
	$(CUSTOM_THIRD_PATH)/dmg_fp/lib/dmg_fp.a $(LIB_DIR)/base/lib/mybase.a $(LIB_DIR)/third_party/lib/libhiredis.a
	$(CXX) $(CPPFLAGS) $(CXXFLAGS) $(LINK_PATHS) -lpthread -lexpat -lrt -lpthread -ldl -liconv -lmysqlclient -lcurl \
	$^ -o $@ 
	ln -sf libmarket.so.2.1 $(SO_DIR)/../market_data/libmarket.so
        
